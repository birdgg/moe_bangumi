{
	"$schema": "https://turbo.build/schema.json",
	"globalEnv": ["NODE_ENV"],
	"ui": "tui",
	"tasks": {
		"//#lint": {},
		"build": {
			"dependsOn": ["server#db:generate", "^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**", "dist/**", "out/**"]
		},
		"//#dev:static": {},
		"dev": {
			"dependsOn": ["server#db:generate"],
			"cache": false,
			"persistent": true
		},
		"server#db:generate": {
			"cache": false
		},
		"db:push": {
			"cache": false,
			"interactive": true
		},
		"db:migrate": {
			"cache": false
		},
		"db:studio": {
			"persistent": true
		},
		"test": {},
		"test:watch": {
			"cache": false,
			"persistent": true
		}
	}
}
